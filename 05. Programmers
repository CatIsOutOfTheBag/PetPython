# -------------------------------------------#
# Операторы, переменные, типы данных, условия
# -------------------------------------------#

# Склонение слова "программист"
a = int(input())
word = "программист"
if 11 <= a % 100 < 20:
    print(a, word + "ов")
elif a % 10 == 1:
    print(a, word)
elif a % 10 in [2, 3, 4]:
    print(a, word + "а")
elif a % 10 in [0, 5, 6, 7, 8, 9]:
    print(a, word + "ов")
