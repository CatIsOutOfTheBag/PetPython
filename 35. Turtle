# 35 Дрессированная черепаха ходит
# север 10
# запад 20
# юг 30
# восток 40

steps = int(input())
x = 0
y = 0
while steps:
    line = input().split()
    if line[0] == 'север':
        y += int(line[1])
    elif line[0] == 'юг':
        y -= int(line[1])
    elif line[0] == 'восток':
        x += int(line[1])
    else:
        x -= int(line[1])
    steps -= 1

print(x, end=' ')
print(y, end=' ')
